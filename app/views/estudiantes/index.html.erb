<h1 align="center">Estudiantes</h1><%= link_to 'Nuevo Estudiante', new_estudiante_path %>
<div class="row col-md-offset-1 col-md-10">
  <table class="table">
    <thead>
      <tr>
        <th>DNI</th>
        <th>Paterno</th>
        <th>Materno</th>
        <th>Nombre</th>
        <th>Teléfono</th>
        <th>Promoción</th>
        <th>Estado</th>
        <th>Departamento</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @estudiantes.each do |estudiante| %>
        <tr>
          <td><%= estudiante.dni %></td>
          <td><%= estudiante.paterno %></td>
          <td><%= estudiante.materno %></td>
          <td><%= estudiante.nombre %></td>
          <td><%= estudiante.telefono %></td>
          <td><%= estudiante.promocion %></td>
          <td><%= estudiante.estado %></td>
          <td><%= estudiante.departamento %></td>
          <td><%= link_to 'Ver', estudiante %></td>
          <td><%= link_to 'Editar', edit_estudiante_path(estudiante) %></td>
          <td><%= link_to 'Eliminar', estudiante, method: :delete, data: { confirm: 'Seguro?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>


<br>


