<h1 align="center">Inscripciones</h1> <%= link_to 'Nueva Inscripcion', new_inscripcion_path %>
<div class="row col-md-offset-1 col-md-10">
  <table class="table">
    <thead>
      <tr>
        <th>Fecha</th>
        <th>Estudiante</th>
        <th>Curso</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @inscripcions.each do |inscripcion| %>
        <tr>
          <td><%= inscripcion.fecha %></td>
          <td><%= inscripcion.estudiante %></td>
          <td><%= inscripcion.curso %></td>
          <td><%= link_to 'Ver', inscripcion %></td>
          <td><%= link_to 'Editar', edit_inscripcion_path(inscripcion) %></td>
          <td><%= link_to 'Eliminar', inscripcion, method: :delete, data: { confirm: 'Seguro?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>
</div>



